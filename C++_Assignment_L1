Question 5
#include <iostream>
#include <cmath> // Include the cmath header for the constant pi

int main() {
    const double pi = 3.141592653589793; // Define the constant pi
    double radius = 10.0; // Radius of the circle

    // Calculate the circumference using the formula: C = 2 * pi * radius
    double circumference = 2 * pi * radius;

    // Output the result
    std::cout << "The circumference of the circle with radius " << radius << " is: " << circumference << std::endl;

    return 0;
}
